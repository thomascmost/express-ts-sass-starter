{
  "name": "express-ts-sass-starter",
  "version": "2.0.0",
  "description": "framework agnostic starter app",
  "main": "app.js",
  "scripts": {
    "postinstall": "npm run build",
    "build": "tsc && webpack --env.production --optimize-minimize",
    "start": "node ./dist/server.js",
    "go": "concurrently \"webpack --watch\" \"npm run start\"",
    "test": "jest"
  },
  "author": "Thomas Constantine Moore",
  "license": "ISC",
  "engines": {
    "node": "7.2.1",
    "npm": "4.0.x"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/*.test\\.(ts|tsx|js)$"
  },
