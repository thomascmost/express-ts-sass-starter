    "@types/q": "1.0.0",
    "@types/typescript": "2.0.0",
    "bcrypt": "1.0.3",
    "body-parser": "^1.19.0",
    "bourbon": "4.3.4",
    "bourbon-neat": "2.0.0",
    "browser-sync": "^2.26.7",
    "connect-flash": "0.1.1",
    "cookie-parser": "1.4.3",
    "core-js": "^2.6.9",
    "css-loader": "0.26.1",
    "express": "^4.17.1",
    "express-domain-middleware": "0.1.0",
    "express-session": "^1.16.2",
    "fs": "0.0.1-security",
    "history": "^4.9.0",
    "lodash.debounce": "^4.0.8",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "ng-inline-svg": "3.0.0",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "path": "0.12.7",
